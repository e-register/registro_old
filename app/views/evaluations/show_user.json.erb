[
<% k = 0 %>
<% @evaluations.each do |e| %>
	{
		"teacher": {
			"id": <%= e.teacher.id %>,
			"name": "<%= e.teacher.name %>",
			"surname": "<%= e.teacher.surname %>"
		},
		"student": {
			"id": <%= e.student.id %>,
			"name": "<%= e.student.name %>",
			"surname": "<%= e.student.surname %>"
		},
		"date": "<%= e.date %>",
		"score": {
			"id": <%= e.score.id %>,
			"value": <%= e.score.value %>,
			"text": "<%= e.score.text %>"
		},
		"subject": {
			"id": <%= e.subject.id %>,
			"name": "<%= e.subject.name %>"
		},
		"type": "<%= e.evaluation_type %>",
		"description": "<%= e.description %>"<% if not e.class_test.nil? %>,
		"class_test": {
			"id": <%= e.class_test.id %>,
			"description": "<%= e.class_test.description %>"
		}<% end %>
	}<%= "," if k < @evaluations.length-1 %>
<% k += 1 %>
<% end %>
]
